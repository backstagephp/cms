{
    "name": "backstage/media",
    "description": "This is my package media",
    "keywords": [
        "vormkracht10",
        "laravel",
        "media"
    ],
    "homepage": "https://github.com/backstagephp/media",
    "support": {
        "issues": "https://github.com/backstagephp/media/issues",
        "source": "https://github.com/backstagephp/media"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Baspa",
            "email": "hello@baspa.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "backstage/fields": "self.version"
    },
    "require-dev": {
        "nunomaduro/larastan": "^2.0.1|^3.0",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "laravel/pint": "^1.24",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Backstage\\Media\\": "src/",
            "Backstage\\Media\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Backstage\\Media\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Backstage\\Media\\MediaServiceProvider"
            ],
            "aliases": {
                "Media": "Backstage\\Media\\Facades\\Media"
            }
        }
    },
    "repositories": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
